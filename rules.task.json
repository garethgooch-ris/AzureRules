{
  "type": "Task",
  "rules": [
    {
      "ifChildState": "Blocked",
      "setParentStateTo": "Blocked",
      "allChildren": false
    },
    {
      "ifChildState": "Done",
      "setParentStateTo": "Review",
      "allChildren": true
    },
    {
      "ifChildState": "In Progress",
      "setParentStateTo": "In Progress",
      "allChildren": false
    },
    {
     "ifChildState": "Committed",
     "parentState": "Approved",
     "setParentState": "Committed",
     "allChildren": false
    }
  ]
}
